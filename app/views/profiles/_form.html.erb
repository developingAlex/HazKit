<%= form_with(model: profile, local: true) do |form| %>
  <% if profile.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(profile.errors.count, "error") %> prohibited this profile from being saved:</h2>

      <ul>
      <% profile.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :user_id %>
    <%= form.text_field :user_id, id: :profile_user_id %>
  </div>

  <div class="field">
    <%= form.label :name %>
    <%= form.text_field :name, id: :profile_name %>
  </div>

  <div class="field">
    <%= form.label :home_location %>
    <%= form.text_field :home_location, id: :profile_home_location %>
  </div>

  <div class="field">
    <%= form.label :profile_image_data %>
    <%= form.text_area :profile_image_data, id: :profile_profile_image_data %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
